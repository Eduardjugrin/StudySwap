<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<?import javafx.scene.effect.InnerShadow?>
<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.StudySwap.graphiccontroller.MessageBoxGUIController"
            prefHeight="200.0" prefWidth="250.0" stylesheets="@../values/style.css">

    <Label fx:id="messageText" alignment="CENTER" contentDisplay="CENTER" prefWidth="350.0" prefHeight="97.0" text = "Error Message" textAlignment="CENTER">
        <font>
            <Font size="15"/>
        </font>
    </Label>
    <Button fx:id="ErrorButton" layoutX="133.0" layoutY="138.0" onAction="#close" mnemonicParsing="false"  prefHeight="35.0" prefWidth="87.0" stylesheets="@../values/style.css" text="OK">
        <font>
            <Font size="14.0"/>
        </font>
    </Button>
    <effect>
        <InnerShadow height="10.0" radius="4.5" width="10.0"/>
    </effect>
</AnchorPane>
